			<ol class="breadcrumb">
				<li><a href="#">
					<em class="fa fa-home"></em>
				</a></li>
				<li class="active">Dashboard</li>
			</ol>
		</div><!--/.row-->
		
		<div class="row">
			<div class="col-lg-12">
				<h1 class="page-header">Dashboard</h1>
			</div>
		</div><!--/.row-->

		<!-- Chart Suhu -->
		<div class="row">
			<div class="col-md-12">
				<div class="panel panel-default">
					<div class="panel-heading">
						Monitoring Suhu & Kelembapan Ruangan
						<span class="pull-right clickable panel-toggle panel-button-tab-left"><em class="fas fa-chevron-up"></em></span></div>
					<div class="panel-body">
						<div class="row">
							<div class="col-md-9 col-sm-12">
								<div class="canvas-wrapper">
									<canvas id="hidro_temp_chart"></canvas>
								</div>
							</div>
							<div class="col-md-3 col-sm-12">
								<h3 class="current-judul">Data Realtime</h3>
								<div class="space"></div>
								<div class="row">
									<div class="col-md-3 col-sm-3 col-xs-3">
										<div class="current-icon">
											<i class="fas fa-thermometer-half fa-2x"></i>
										</div>
									</div>
									<div class="col-md-9 col-sm-9 col-xs-9">
										<div class="current">
											<p class="current-category">Suhu Ruangan</p>
											<span id="hidro_temp_value" class="current-value">0</span>
											<span class="current-title"> &deg;C</span>
										</div>
									</div>
								</div>
								<div class="clearfix"></div>
								<div class="row">
									<div class="col-md-3 col-sm-3 col-xs-3">
										<div class="current-icon">
											<i class="fas fa-thermometer-half fa-2x"></i>
										</div>
									</div>
									<div class="col-md-9 col-sm-9 col-xs-9">
										<div class="current">
											<p class="current-category">Kel. Ruangan</p>
											<span id="hidro_hum_value" class="current-value">0</span>
											<span class="current-title"> %</span>
										</div>
									</div>
								</div>
								<br>
								<div class="row">
									<div class="col-md-12 btn-detail-wrapper">
										<a href="/details-ruangan" class="btn btn-sm btn-primary btn-detail"><em class="fas fa-search">&nbsp;</em> Detail</a>
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
